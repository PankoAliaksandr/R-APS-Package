\name{plot_active_relative_weights}
\alias{plot_active_relative_weights}
\title{plot_active_relative_weights}
\usage{plot_active_relative_weights(rw_act_v,
                    tickers_act_v,
                    txtsize,
                    lb_act_v,
                    ub_act_v}
\description{plots optimization results}
\arguments{
  \item{rw_act_v}{optimal relative weights}
  \item{tickers_act_v}{covariance data frame}
  \item{txtsize}{convictions}
  \item{lb_act_v}{lower bound}
  \item{ub_act_v}{upper bound}
  }
